{"ast":null,"code":"export default {\n  data() {\n    let departmentId = this.$route.query.id;\n    return {\n      departmentData: {},\n      departmentId: departmentId\n    };\n  },\n  mounted() {\n    this.loadDepartment();\n  },\n  methods: {\n    loadDepartment() {\n      this.$request.get('/department/selectById/' + this.departmentId).then(res => {\n        if (res.code === '200') {\n          this.departmentData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    init() {\n      this.fromVisible = true;\n    },\n    save() {\n      let data = {\n        userId: this.user.id,\n        departmentId: this.departmentId,\n        description: this.description\n      };\n      this.$request.post('/application/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('Success, Waiting for Approval');\n          this.fromVisible = false;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","departmentId","$route","query","id","departmentData","mounted","loadDepartment","methods","$request","get","then","res","code","$message","error","msg","init","fromVisible","save","userId","user","description","post","success"],"sources":["src/views/front/DepartmentDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 50%; margin: 20px auto\">\r\n      <div style=\"color: #333333; font-size: 20px; font-weight: 700\">{{ departmentData.clubName }}：Club Intro\r\n        <el-button type=\"primary\" @click(\"init\")>Apply</el-button>\r\n      </div>\r\n      <div style=\"margin-top: 10px; color: #767474\">Publish Date：{{ departmentData.time }}</div>\r\n      <div style=\"margin-top: 30px\" v-html=\"departmentData.description\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    let departmentId = this.$route.query.id\r\n    return {\r\n      departmentData: {},\r\n      departmentId: departmentId\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadDepartment()\r\n  },\r\n  methods: {\r\n    loadDepartment() {\r\n      this.$request.get('/department/selectById/' + this.departmentId).then(res => {\r\n        if (res.code === '200') {\r\n          this.departmentData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    init() {\r\n      this.fromVisible = true\r\n    },\r\n    save() {\r\n      let data = {\r\n        userId: this.user.id,\r\n        departmentId: this.departmentId,\r\n        description: this.description\r\n      }\r\n      this.$request.post('/application/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('Success, Waiting for Approval')\r\n          this.fromVisible = false\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAcA;EAEAA,KAAA;IACA,IAAAC,YAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,cAAA;MACAJ,YAAA,EAAAA;IACA;EACA;EACAK,QAAA;IACA,KAAAC,cAAA;EACA;EACAC,OAAA;IACAD,eAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,kCAAAT,YAAA,EAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAR,cAAA,GAAAO,GAAA,CAAAZ,IAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,KAAA;MACA,KAAAC,WAAA;IACA;IACAC,KAAA;MACA,IAAAnB,IAAA;QACAoB,MAAA,OAAAC,IAAA,CAAAjB,EAAA;QACAH,YAAA,OAAAA,YAAA;QACAqB,WAAA,OAAAA;MACA;MACA,KAAAb,QAAA,CAAAc,IAAA,qBAAAvB,IAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAU,OAAA;UACA,KAAAN,WAAA;QACA;UACA,KAAAJ,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}